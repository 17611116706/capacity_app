<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.schoolapp">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/StartTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".mvp.ui.activity.mine.MiscellaneousActivity"></activity>
        <activity android:name=".mvp.ui.activity.mine.CollaborateCenterActivity" />
        <activity android:name=".mvp.ui.activity.mine.PersionCenterActivity" />
        <activity android:name=".mvp.ui.activity.mine.SettingActivity" />
        <activity android:name=".mvp.ui.activity.login.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mvp.ui.activity.home.TodayListActivity" />
        <activity android:name=".mvp.ui.activity.login.LoginSetting" />
        <activity android:name=".mvp.ui.activity.login.LoginActivity" />
        <activity
            android:name=".mvp.ui.activity.MainActivity"
            android:launchMode="singleTask" />

        <meta-data
            android:name="design_width_in_dp"
            android:value="375" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>